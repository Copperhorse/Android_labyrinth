{
BitmapFont: {
	cinzel: {
		file: cinzel.fnt
		scaledSize: 48
		markupEnabled: false
		flip: false
	}
}
Color: {
	black: {
		hex: 000000ff
	}
	gray: {
		hex: aeaeaeff
	}
	white: {
		hex: ffffffff
	}
}
TintedDrawable: {
	round-dark-gray: {
		name: round-white
		color: {
			hex: 383838ff
		}
	}
	round-light-gray: {
		name: round-white
		color: {
			hex: dcdcdcff
		}
	}
	round-gray-small: {
		name: round-white-small
		color: {
			hex: 767676ff
		}
	}
	round-black-small: {
		name: round-white-small
		color: {
			hex: 000000ff
		}
	}
	gray: {
		name: white
		color: {
			hex: 969696ff
		}
	}
}
ButtonStyle: {
	default: {
		up: round-white
		down: round-white
		over: round-white
	}
	toggle: {
		up: round-gray
		down: round-dark-gray
		over: round-light-gray
		checked: round-dark-gray
		checkedOver: round-light-gray
	}
}
ImageButtonStyle: {
	default: {
		up: round-gray
		down: round-dark-gray
		over: round-light-gray
	}
	toggle: {
		up: round-gray
		down: round-dark-gray
		over: round-light-gray
		checked: round-dark-gray
		checkedOver: round-light-gray
	}
}
ProgressBarStyle: {
	default-horizontal: {
		background: progress-bar
		knobBefore: progress-bar-fill
	}
	default-vertical: {
		background: progress-bar
		knobBefore: progress-bar-fill-vert
	}
}
ScrollPaneStyle: {
	default: {
		background: list
		hScroll: round-black-small
		hScrollKnob: round-gray-small
		vScroll: round-black-small
		vScrollKnob: round-gray-small
	}
}
SliderStyle: {
	default-horizontal: {
		background: slider-horizontal
		knob: slider-knob
	}
	default-vertical: {
		background: slider-vertical
		knob: slider-knob
	}
}
SplitPaneStyle: {
	default-horizontal: {
		handle: horizontal-split-pane
	}
	default-vertical: {
		handle: vertical-split-pane
	}
}
TextButtonStyle: {
	default: {
		font: cinzel
		fontColor: black
		downFontColor: white
		up: round-white
		down: round-light-gray
		over: round-white
	}
}
TouchpadStyle: {
	default: {
		background: touchpad
		knob: touchpad-knob
	}
}
TreeStyle: {
	default: {
		plus: plus
		minus: minus
	}
}
}
